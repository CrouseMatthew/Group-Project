body {
  font-size: 1.1em;
  font-family: arial, helvetica, sans-serif;
  background-color: #a69e94;

}

h1 {
  margin-left: 1em;
  margin-bottom: 10px;
}

ul {
  padding-left: 25px;
}

nav {
  float: right;
  width: 210px;
  height: 100%;
  color: #efefe4;
  font-size: 11pt;
  text-decoration: none;
  line-height: 27px;
  margin-right: 30px;
  padding-bottom: 5px;
  background-color: #86523D;
  opacity: 0.7;
  margin-left: 5px;
  border:3px solid #602320;
  border-radius:20px;
}

footer {
  width: 54%;
  font-size: .80em;
  min-height: 40px;
  text-align: center;
  margin-left: auto;
  margin-right: auto;
  clear: both;
  background-color: #938072;
}

td {
  padding-left: 10px;
}

a:link {color:#efefe4;}      /* unvisited link */
a:visited {color:#DBDBC1;}  /* visited link */
a:hover {color:#2D2C26;}   /* mouse over link */
a:active {color:#F9F9F7;} /* selected link */



